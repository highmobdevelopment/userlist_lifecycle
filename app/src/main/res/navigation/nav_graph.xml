<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:startDestination="@+id/listFragment">

    <fragment
        android:id="@+id/listFragment"
        android:name="erevacation.com.userlistlifecycle.ui.homescreen.list.ListFragment"
        android:label="ListFragment">
        <action
            android:id="@+id/action_listFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="erevacation.com.userlistlifecycle.ui.homescreen.profile.ProfileFragment"
        android:label="ProfileFragment" />

</navigation>